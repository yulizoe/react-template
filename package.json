{
  "name": "react-scaffold",
  "version": "1.0.0",
  "description": "react-scaffold",
  "main": "index.js",
  "engines": {
    "node": ">=7.5.0",
    "npm": ">=4.1.2"
  },
  "author": "vnot <weinotme@gmail.com>",
  "license": "MIT",
  "scripts": {
    "install:gfw": "npm i cross-env && cross-env PHANTOMJS_CDNURL=http://npm.taobao.org/mirrors/phantomjs npm install",
    "clean": "rimraf ./dist",
    "compile": "better-npm-run compile",
    "lint": "eslint ./app",
    "lint:fix": "npm run lint -- --fix",
    "lint:css": "stylelint app/**/*.css",
    "start": "better-npm-run start",
    "dev": "better-npm-run dev",
    "test": "better-npm-run test",
    "test:dev": "npm run test -- --browsers=Chrome --single-run=false",
    "build": "better-npm-run build",
    "server": "http-server ./dist -p 8000 -o",
    "lock": "npm shrinkwrap --dev"
  },
  "betterScripts": {
    "compile": {
      "command": "babel-node dev/bin/compile"
    },
    "dev": {
      "command": "nodemon --exec babel-node dev/bin/server",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "build": {
      "command": "npm run clean && npm run compile",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "start": {
      "command": "npm run dev"
    },
    "test": {
      "command": "babel-node ./node_modules/karma/bin/karma start dev/bin/karma",
      "env": {
        "NODE_ENV": "test"
      }
    }
  },
